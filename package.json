{
  "name": "astro-api",
  "version": "1.0.0",
  "description": "Simple API for Resources in Astroneer",
  "main": "index.js",
  "scripts": {
    "migrate": "npx prisma migrate dev --name init",
    "seed": "node be/scripts/insert.js",
    "setup": "npm i && npm run migrate && npm run seed && cd fe && npm i",
    "be": "node --watch be/index.js",
    "fe": "npm --prefix fe start"
  },
  "author": "KovKor",
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "express": "^4.18.3",
    "prettier": "^2.2.1",
    "ts-loader": "^9.5.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "prisma": "^5.11.0"
  },
  "prisma": {
    "schema": "be/prisma/schema.prisma"
  }
}
